<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nisira.gcalderon.policesecurity">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name="com.nisira.core.interfaces.VariableGlobal">
        <activity android:name="com.nisira.view.Activity.Login_Activity"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>
        <activity
            android:name="com.nisira.view.Activity.NavigationPolice_Activity"
            android:label="@string/title_activity_navigation_police"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.nisira.view.Activity.SplashScreen_Activity"
            android:label="SplashScreen"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>